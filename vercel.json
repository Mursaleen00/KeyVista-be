{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/docs(/(.*))?",
      "dest": "src/main.ts",
      "methods": ["GET"]
    },
    {
      "src": "/api(/(.*))?",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ]
}
